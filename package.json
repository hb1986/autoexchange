{
  "name": "autoexchange",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:test": "cross-env NODE_ENV=test nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "prepare": "husky install",
    "generate": "prisma format && prisma generate",
    "dbpush": "yarn generate && prisma db push",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@elastic/elasticsearch": "6",
    "@nestjs-modules/ioredis": "^1.0.1",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^9.1.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.1.1",
    "@nestjs/elasticsearch": "^9.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.1.1",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/terminus": "^9.1.1",
    "@nestjs/throttler": "^3.0.0",
    "@prisma/client": "4.11.0",
    "bcryptjs": "^2.4.3",
    "bignumber.js": "^9.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.5",
    "discord.js": "^13.8.1",
    "express-session": "^1.17.3",
    "fast-csv": "^4.3.6",
    "graphql": "^16.3.0",
    "graphql-request": "^4.2.0",
    "ioredis": "^5.2.3",
    "ipfs-http-client": "^56.0.1",
    "mailgun.js": "^8.0.6",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.40",
    "nestjs-ethers": "^1.0.3",
    "nodemailer": "^6.8.0",
    "objects-to-csv": "^1.3.6",
    "openai": "^3.1.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "prisma": "^4.11.0",
    "prisma-client": "^0.0.0",
    "read-excel-file": "^5.5.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "string-random": "^0.1.3",
    "web3-utils": "^1.8.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.3",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/testing": "^9.1.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/jest": "29.0.3",
    "@types/lodash": "^4.14.185",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^7.0.4",
    "jest": "^29.0.3",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^29.0.1",
    "ts-loader": "^9.4.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
